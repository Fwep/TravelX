{
   "name": "travelx",
   "version": "1.0.0",
   "description": "A travel recommendation site that suggests pre-bundled packages",
   "main": "app.js",
   "scripts": {
      "start": "npm run clean && npm run build && node ./server/dist/app.js",
      "build:server": "babel ./server/src -d ./server/dist",
      "build:client": "webpack -p",
      "clean": "rimraf ./client/dist && rimraf ./server/dist",
      "build": "npm run build:server && npm run build:client",
      "dev": "concurrently -r -k \"npm run build:dev-server\" \"npm run start:dev-server\" \"npm run client:dev-server\" ",
      "client:dev-server": "webpack-dev-server --mode=development",
      "build:dev-server": "npm run build:server && babel ./server/src -w -d ./server/dist",
      "start:dev-server": "nodemon ./server/dist/app.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Fwep/TravelX.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Fwep/TravelX/issues"
   },
   "homepage": "https://github.com/Fwep/TravelX#readme",
   "dependencies": {
      "@babel/cli": "^7.2.3",
      "@babel/core": "^7.2.2",
      "@babel/polyfill": "^7.2.5",
      "@babel/preset-env": "^7.2.3",
      "@babel/preset-react": "^7.0.0",
      "axios": "^0.18.0",
      "babel-loader": "^8.0.4",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.3",
      "cheerio": "^1.0.0-rc.2",
      "express": "^4.16.4",
      "file-loader": "^3.0.1",
      "html-loader": "^0.5.5",
      "html-webpack-plugin": "^3.2.0",
      "jwt-decode": "^2.2.0",
      "mongoose": "^5.4.1",
      "morgan": "^1.9.1",
      "passport": "^0.4.0",
      "passport-jwt": "^4.0.0",
      "react": "^16.7.0",
      "react-dom": "^16.7.0",
      "react-redux": "^6.0.0",
      "react-router-dom": "^4.3.1",
      "redux": "^4.0.1",
      "redux-thunk": "^2.3.0",
      "request": "^2.88.0",
      "request-promise": "^4.2.2",
      "rimraf": "^2.6.2",
      "styled-components": "^4.1.3",
      "validator": "^10.10.0",
      "webpack": "^4.28.2",
      "webpack-cli": "^3.1.2"
   },
   "devDependencies": {
      "babel-plugin-styled-components": "^1.10.0",
      "concurrently": "^4.1.0",
      "nodemon": "^1.18.9",
      "redux-logger": "^3.0.6",
      "webpack-dev-server": "^3.1.14"
   }
}
